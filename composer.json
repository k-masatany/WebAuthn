{
  "name": "k-masatany/web-authn",
  "description": "WebAuthn Server for Laravel 5",
  "keywords": [
    "laravel",
    "webauthn"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "k-masatany",
      "email": "sonntag902@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "KMasatany\\WebAuthn\\": "src/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "autoload-dev": {
    "psr-4": {
      "KMasatany\\WebAuthn\\Tests\\": "tests/"
    }
