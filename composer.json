  "minimum-stability": "dev",
  "require": {
    "php": "^7.0",
    "laravel/framework": "5.7.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1@dev"
  },
  "extra": {
    "laravel": {
      "providers": [
        "KMasatany\\WebAuthn\\WebAuthnServiceProvider"
      ]
    }
  }
}
